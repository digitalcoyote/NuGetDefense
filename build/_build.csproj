<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net6.0</TargetFramework>
        <RootNamespace></RootNamespace>
        <NoWarn>CS0649;CS0169</NoWarn>
        <NukeRootDirectory>..</NukeRootDirectory>
        <NukeScriptDirectory>..\Src</NukeScriptDirectory>
        <LangVersion>9</LangVersion>
        <Configurations>Debug;Release;DotNetTool</Configurations>
        <Platforms>AnyCPU</Platforms>
    </PropertyGroup>

    <ItemGroup>
        <PackageDownload Include="GitVersion.Tool" Version="[5.1.1]" />
    </ItemGroup>

    <ItemGroup>

      <PackageReference Include="Azure.Identity" Version="1.11.4" />

      <PackageReference Include="Microsoft.Build.Tasks.Core" Version="16.11.6" />

      <PackageReference Include="NuGet.Common" Version="5.11.5" />
        <PackageReference Include="Nuke.Common" Version="7.0.2" />
        <PackageReference Include="System.Drawing.Common" Version="4.7.2" />
        <PackageReference Include="System.Security.Cryptography.Xml" Version="4.7.1" />
        <PackageReference Include="System.Text.Json" Version="8.0.5" />
    </ItemGroup>

</Project>
